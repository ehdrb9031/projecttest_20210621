<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hk.notice">
	<insert id="insertNotice" parameterType="JoinUserDto">
		INSERT INTO NOTICE (no_seq,id,title,content,readcount,regdate)VALUES
		(NOTICE_NO_SEQ.NEXTVAL,#{id},#{title},#{content},#{readcount},SYSDATE)
	</insert>
</mapper>